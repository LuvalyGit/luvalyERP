    private void GrillaMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_GrillaMouseClicked
        
        ExeSql Sql = new ExeSql();
        ResultSet Rs;
        int autorizado = 0;
        
        if(evt.getClickCount()==2){
        
            try {
            
                    Rs = Sql.Select("SELECT * from boletas_garantia \n"+
                                    "WHERE rut ="+Grilla.getValueAt(Grilla.getSelectedRow(),4) + "\n"+
                                    "AND nrodocto = "+ Grilla.getValueAt(Grilla.getSelectedRow(),0) + "\n"+
                                    "AND femision = '"+ Grilla.getValueAt(Grilla.getSelectedRow(),2) + "'\n"+
                                    "ORDER by femision DESC");
            
                    if (Sql.GetRowCount() > 0){
            
                        Rs.next();
                
                            autorizado = Rs.getInt("autoriza");
                
                            if (autorizado == 0){
                
                                btAutorizar.setEnabled(true);
                
                            }else if (autorizado == 1) {
                
                                 btAutorizar.setEnabled(false);
                
                            }
                            
                            txRut.setText(Grilla.getValueAt(Grilla.getSelectedRow(),4).toString());
                            btIr.setEnabled(true);
                            btIr.doClick();
                            
                            txNroDocto.setText(Grilla.getValueAt(Grilla.getSelectedRow(),0).toString());
                            
                            cbBancoId.setSelectedItem(Rs.getString("banco"));
                            cbBanco.setSelectedIndex(cbBancoId.getSelectedIndex());
                            
                            dtEmision.setDate(Rs.getDate("femision"));
                            dtVencimiento.setDate(Rs.getDate("fvenc"));
                            
                            txTotal.setText(Grilla.getValueAt(Grilla.getSelectedRow(),1).toString().trim());
                            
                            btEditar.setEnabled(true);
                            nuevo = 0;
                    }
            
            
            
            } catch (Exception e) {
                fmMain.Mensaje(e.getMessage());
            } finally{
                Sql.Close();
            }
            
            
        }
        
        
    }//GEN-LAST:event_GrillaMouseClicked
     
    
     private void CargaBoletas(){
        ExeSql Sql = new ExeSql();
        ResultSet Rs;

        int autorizado = 0;
        boolean estado = false;
        
        DefaultTableModel TableModel = (DefaultTableModel) Grilla.getModel();

        
        while(TableModel.getRowCount()>0)
                TableModel.removeRow(0);        
        
        
        try {
            
            Rs = Sql.Select("select * from boletas_garantia order by femision DESC");
            
            if (Sql.GetRowCount() > 0){
            
                while(Rs.next()){
                
                    autorizado = Rs.getInt("autoriza");
                
                    if (autorizado == 0){
                
                        estado = false;
                
                    }else if (autorizado == 1) {
                
                        estado = true;
                
                    }
                
                
                    TableModel.addRow(new Object[]{
                                            Rs.getString("nrodocto").trim(),
                                            Rs.getString("monto").trim(), 
                                            Rs.getDate("femision"),
                                            Rs.getDate("fvenc"),
                                            Rs.getString("rut").trim(),
                                            estado
                    });
                
                               
                }
                
             }
            
        } catch (Exception e) {
            fmMain.Mensaje(e.getMessage());
        } finally{
            Sql.Close();
        }
    }
    
    private void CargaBancos(){
        ExeSql Sql = new ExeSql();
        ResultSet Rs;

        cbBanco.removeAllItems();
        cbBancoId.removeAllItems();

        try {
            Rs = Sql.Select("select codigo,nombre\n" +
                            "from par_general \n" +
                            "where tipo='BANCOS'\n" +
                            "and codigo IN ('46') \n" +
                            "and vigente=1\n" +
                            "order by codigo");
            
            while(Rs.next()){
                cbBancoId.addItem(Rs.getString("codigo"));
                cbBanco.addItem(Rs.getString("nombre"));
                               
            }
            
         
            
        } catch (Exception e) {
            fmMain.Mensaje(e.getMessage());
        } finally{
            Sql.Close();
        }
    }
    
   
    
    private void CargaCliente(String Rut){
        ExeSql Sql = new ExeSql();
        ResultSet Rs;
        
        try {
            Rs = Sql.Select("select Rut,nombre \n" +
                            "from cliente\n" +
                            "where rut=" + Rut);
            Rs.next();
            
            if(Sql.GetRowCount()>0){
                
                txRut.setText(Rs.getString("Rut"));
                txNombre.setText(Rs.getString("Nombre"));
                
            }else {
                JOptionPane.showMessageDialog(null, "Cliente con RUT: "+Rut+" no existe.");
                return;
                
            }
            
        } catch (Exception e) {
            JOptionPane.showMessageDialog(null, e.getMessage());
            System.out.println(e.getMessage());
        } finally{
            Sql.Close();
        }
    }
    
    
    
