    private void GrillaMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_GrillaMouseClicked

     String nombre;   
     lstClientes.setSelectionMode(ListSelectionModel.SINGLE_SELECTION );
     int j=0;   
        if(evt.getClickCount()==2 ){
            
                nombre = Grilla.getValueAt(Grilla.getSelectedRow(), 2).toString().trim();    
                for (int i=0; i<= lstClientes.getModel().getSize()  ; i++){
                   
                    if (nombre.equals( lstClientes.getModel().getElementAt(i).toString())){
                        j=i;
                    break;    
                    }
                        
                }
                lstClientes.setSelectedIndex(j);
                lstClientes.ensureIndexIsVisible(lstClientes.getSelectedIndex());
                
                lstRut.setSelectedIndex(j);
                lstRut.ensureIndexIsVisible(lstRut.getSelectedIndex());
                
                CargaOC();
                
                    }
                    
            
    }//GEN-LAST:event_GrillaMouseClicked

