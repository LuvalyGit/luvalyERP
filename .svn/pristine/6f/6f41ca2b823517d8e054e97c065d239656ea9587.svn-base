    public void Cargalst(JList lista ,JList id, String sql ){
        DefaultListModel value;
        DefaultListModel valueid;
        ExeSql Sql = new ExeSql();
        ResultSet Rs;
        String Query="";
        lista.removeAll();
        int i=0;
        
//        CboProveedor.addItem("<Todos>");  
//        CboProveedor.setSelectedIndex(0);
        value =new DefaultListModel();
        valueid =new DefaultListModel();
        try {
           
            
            Query="select distinct o.rut,    trim(c.nombre) as clientes \n" +
                "from occh o\n" +
                "left join cliente c on c.rut=o.rut\n" +
                "inner  join personal p on p.usuario = '" + fmMain.GetUsuario() + "'  \n" +
                "inner join cli_cartera_venta cv on cv.rut_cliente=o.rut\n" +    
                "where \n" +
                " o.femision is not null ";    
                
                  if (fmMain.trae_nivel(fmMain.GetUsuario())<80) {
                    Query = Query + " and cv.cod_vendedor=p.vendedor ";
                } else {
                    Query = Query + " and cv.cod_vendedor>0";
                }
                Query = Query + " order by clientes";
            System.out.println (Query);
            Rs = Sql.Select(Query);
            if(Sql.GetRowCount()==0) return;
             value.addElement(new ComboCodigos("Seleccione",0)); 
             valueid.addElement(new ComboCodigos("Seleccione",0)); 
           while(Rs.next()){
            value.addElement(new ComboCodigos(Rs.getString("clientes").trim(),Rs.getInt("rut")));
            valueid.addElement(new ComboCodigos(Rs.getString("rut").trim(),Rs.getInt("rut")));
            //System.out.println(Rs.getString("clientes").trim());
            i++;
           } 
           lista.setModel(value);
           id.setModel(valueid);
           
           
           if (lista.getSelectedIndex()>0){
                    lista.setSelectedIndex(0);
                    id.setSelectedIndex(0);
           }
           
        } catch (Exception e) {
            System.out.println(e);
        }
        finally{
            Sql.Close();
        }
     }
    
    
