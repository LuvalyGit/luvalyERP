    private void CargaGrilla(){
         ExeSql Sql = new ExeSql();
        ResultSet Rs;
        String Query="";
        double Total =0;
        DefaultTableModel TableModel = (DefaultTableModel) Grilla.getModel();
        
        int ContReg=0;
//        int intNivel =0;
//        intNivel = fmMain.trae_nivel(fmMain.GetUsuario());
        
        
        
        
        if (cboAgno.getSelectedIndex()<=0 ){
            return;
        }
        
        
        
        while(TableModel.getRowCount()>0)
           TableModel.removeRow(0);        
try {

    
    Query= "";
   
 
        Query = Query +   
                        " select \n" +
                        "cast(o.femision as date) as femision, \n" +
                        "o.rut,\n" +
                        "trim(c.nombre) as nombreCliente,\n" +
                        "o.codigo_oc cc , o.orden as orden, \n" +
                        "pr.sku,pr.nombre , d.cantidad, d.valorunitario,\n" +
                        "d.cantidad * d.valorunitario as SubTotal\n" +
                        "from occh o\n" +
                        "left join occhdet d on o.rut=d.rut and o.codigo_oc=d.codigo_oc and o.orden=d.orden\n" +
                        "left join cliente c on c.rut=o.rut\n" +
                        "left join personal p on p.usuario = '" + fmMain.GetUsuario() + "' \n";
                        //if ((cbo_cliente.getSelectedIndex() + cbo_ccosto.getSelectedIndex() + cbo_Ordenes.getSelectedIndex() >0)){    
                            
        
                        if (CboVendedor.getSelectedIndex()>0 ||  intNivel <80   ) {
                            Query = Query + "inner join cli_cartera_venta cv on cv.rut_cliente=o.rut\n";
                        }     
                        //}
                        Query = Query + " inner join producto pr \n" +
                        " on d.sku = pr.sku   Where femision is not null \n";
                        if (lstClientes.getSelectedIndex() >0){
                               
                               Query = Query + " and o.rut = " + lstRut.getModel().getElementAt(lstRut.getSelectedIndex()).toString();
                        }      

                        if (cbo_ccosto.getSelectedIndex() >0){
                               Query = Query + " and o.codigo_oc=" + cbo_ccosto.getSelectedItem().toString();
                        }    
                        
                        if (cbo_Ordenes.getSelectedIndex() >0){
                               Query = Query + " and o.orden = '" + cbo_Ordenes.getSelectedItem().toString() + "'" ;
                        }    
                        
                        
//                         if (!fmMain.GetUsuarioAdministrador()) {
//                            Query = Query + " and cv.cod_vendedor=p.vendedor ";
//                        } else {
//                            Query = Query + " and cv.cod_vendedor>0";
//                        }
                        
                        if (intNivel<80 && chkVtas.isSelected()  ) {
                            Query = Query + " and cv.cod_vendedor= p.vendedor and o.vendedor=p.vendedor ";
                        }
                        else if (intNivel<80 && !chkVtas.isSelected()  )  {
                                    Query = Query + " and cv.cod_vendedor = p.vendedor";
                                }
                        
                        
                        
                        if (intNivel>=80 ) {
                                if (CboVendedor.getSelectedIndex()>0   &&   !chkVtas.isSelected()  ) {
                                        ComboCodigos VendCbo = (ComboCodigos) CboVendedor.getSelectedItem();
                                        Query = Query + " and cv.cod_vendedor =" +  VendCbo.getId()  ;
                                    }
                                else if (CboVendedor.getSelectedIndex()>0   &&   chkVtas.isSelected()){
                                     ComboCodigos VendCbo = (ComboCodigos) CboVendedor.getSelectedItem();
                                         Query = Query + " and cv.cod_vendedor = " +  VendCbo.getId() + "   and o.vendedor = " + VendCbo.getId() ;}
                                    
                                
                            
                        }
                        
                Query = Query + " and  extract(year from femision)= " + cboAgno.getSelectedItem().toString() + "\n" +
                  " and extract(month from femision)=" + cboMes.getSelectedItem().toString() + "\n";

                         
                         
                         
                         
  // EJECUTA CONSULTA
            Rs = Sql.Select(Query);
            
            
            System.out.println(Query);
            if(Sql.GetRowCount()==0) {
             fmMain.Mensaje("No existen registros");
             lblReg.setText(String.valueOf(0));
             lbTotal.setText(fmMain.FormatoTotal(0));
                return;          
            }
            
            while(Rs.next()){
               ContReg++;
              TableModel.addRow(new Object[]{  
                                               Rs.getString("femision"), 
                                               Rs.getString("rut"), 
                                               Rs.getString("nombreCliente").trim(), 
                                               Rs.getString("cc"),
                                               Rs.getString("orden"),
                                               Rs.getString("sku"),
                                               Rs.getString("nombre"),
                                               fmMain.SetGuardar(Rs.getString("cantidad")),
                                               fmMain.FormatoNumero(Rs.getDouble("valorunitario")),
                                               fmMain.FormatoTotal(Rs.getDouble("SubTotal"))});
              
              Total = Total + Rs.getDouble("SubTotal");
       }
         lblReg.setText(String.valueOf(ContReg));
         lbTotal.setText(fmMain.FormatoTotal(Total));
         
    }
catch (Exception e) {
            fmMain.Mensaje("Existe una inconsistencia en la información.");
            lblReg.setText(String.valueOf(0));
            lbTotal.setText(fmMain.FormatoTotal(0));
        }finally{
            Sql.Close();
        }
    }
    
    

    
       
    
    
    

