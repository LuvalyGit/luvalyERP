    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        // TODO add your handling code here:
     String StrRut ="";   
     int vend =-1;
          jd_RepoVtas Ubicacion = new jd_RepoVtas(null, true);
          
          if (lstClientes.getSelectedIndex()>0){
              
              StrRut = lstRut.getModel().getElementAt(lstRut.getSelectedIndex()).toString();
          }
          
          
           if (CboVendedor.getSelectedIndex()>0) {
             ComboCodigos VendCbo = (ComboCodigos) CboVendedor.getSelectedItem();
             vend= VendCbo.getId() ;
        }
          
          
         Ubicacion.CargaReporte(StrRut ,cboMes.getSelectedItem().toString(),cboAgno.getSelectedItem().toString(),  chkVtas.isSelected(),vend);
         Ubicacion.selecciona_reg();
                  
            Ubicacion.setVisible(true);
        
            
//            txtUbicacionOrig.setText(Ubicacion.GetUbicacionCod());
//            txNombreUbicaOrig.setText(Ubicacion.GetUbicacion());
    }//GEN-LAST:event_jButton1ActionPerformed

