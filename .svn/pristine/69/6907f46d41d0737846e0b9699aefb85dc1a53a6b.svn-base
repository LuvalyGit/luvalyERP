    private void btConciliarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btConciliarActionPerformed
        jdConciliacionEntre Concilia = new jdConciliacionEntre(null, true);
        Concilia.setLocationRelativeTo(null);
        if (cbBanco.getSelectedItem().equals(cbBanco2.getSelectedItem()) && cbCuenta.getSelectedItem().equals(cbCuenta2.getSelectedItem()))
            {
            if (chkTarjeta.isSelected())
                {
                    System.out.println("Tipo 1");
                }
            else 
                {
                 System.out.println("Tipo 2");
                }
            }
        else{
             System.out.println("Tipo 3");
        }
        if (GrillaCartola.getValueAt(GrillaCartola.getSelectedRow(),GetColBanco("CARGO")).toString().equals(0))
        {
            if(!GrillaCartola.getValueAt(GrillaCartola.getSelectedRow(),GetColBanco("ABONO")).toString().equals(GrillaCartola2.getValueAt(GrillaCartola2.getSelectedRow(),GetColBanco("CARGO")).toString()))
                    {
                    fmMain.Mensaje("Los montos no coinciden");
                    return;   
                    }
        }
        else{
             if(!GrillaCartola.getValueAt(GrillaCartola.getSelectedRow(),GetColBanco("CARGO")).toString().equals(GrillaCartola2.getValueAt(GrillaCartola2.getSelectedRow(),GetColBanco("ABONO")).toString()))
             {
                   fmMain.Mensaje("Los montos no coinciden");
                    return;   
                 
             }
        }
        
       
        
        Concilia.SetValores(GrillaCartola.getValueAt(GrillaCartola.getSelectedRow(),GetColBanco("FECHA")).toString().trim(),
                                       GrillaCartola.getValueAt(GrillaCartola.getSelectedRow(),GetColBanco("NUMERO")).toString(),
                                       GrillaCartola.getValueAt(GrillaCartola.getSelectedRow(),GetColBanco("DETALLE")).toString(),
                                       GrillaCartola.getValueAt(GrillaCartola.getSelectedRow(),GetColBanco("CARGO")).toString(),
                                       GrillaCartola.getValueAt(GrillaCartola.getSelectedRow(),GetColBanco("ABONO")).toString(),
                                       GrillaCartola.getValueAt(GrillaCartola.getSelectedRow(),GetColBanco("ID")).toString(),
                                       
                                       GrillaCartola2.getValueAt(GrillaCartola2.getSelectedRow(),GetColBanco("FECHA")).toString().trim(),
                                       GrillaCartola2.getValueAt(GrillaCartola2.getSelectedRow(),GetColBanco("NUMERO")).toString(),
                                       GrillaCartola2.getValueAt(GrillaCartola2.getSelectedRow(),GetColBanco("DETALLE")).toString(),
                                       GrillaCartola2.getValueAt(GrillaCartola2.getSelectedRow(),GetColBanco("CARGO")).toString(),
                                       GrillaCartola2.getValueAt(GrillaCartola2.getSelectedRow(),GetColBanco("ABONO")).toString(),
                                       GrillaCartola2.getValueAt(GrillaCartola2.getSelectedRow(),GetColBanco("ID")).toString());
        Concilia.setVisible(true);
        
