    private void CargaBancos(){
        cbBancoId2.setVisible(false);
        cbCuentaId2.setVisible(false);
        cbBancoId.setVisible(false);
        cbCuentaId.setVisible(false);
        ExeSql Sql = new ExeSql();
        ResultSet Rs;
        
        cbBanco2.removeAllItems();
        cbBancoId2.removeAllItems();
        cbBanco.removeAllItems();
        cbBancoId.removeAllItems();
        
        try {
            Rs = Sql.Select("select id,nombre\n" +
                            "from bco_bancos where id>0\n" +
                            "order by id");
            while(Rs.next()){
                cbBancoId.addItem(Rs.getInt("id"));
                cbBanco.addItem(Rs.getString("nombre"));
                cbBancoId2.addItem(Rs.getInt("id"));
                cbBanco2.addItem(Rs.getString("nombre"));
            }
            cbBanco.setSelectedIndex(-1);
            cbBancoId.setSelectedIndex(-1);
            cbBanco2.setSelectedIndex(-1);
            cbBancoId2.setSelectedIndex(-1);
        } catch (Exception e) {
            fmMain.Mensaje("Error");
        } finally{
            Sql.Close();
        }
    }
    private void CargaCuentas(int cual){
        ExeSql Sql = new ExeSql();
        ResultSet Rs;
        String cbBancoID_dato;
        
        if (cual ==2)
            {
                cbBancoID_dato = cbBancoId2.getSelectedItem().toString().trim();
                cbCuenta2.removeAllItems();
                cbCuentaId2.removeAllItems();
            
            }
        else{
            
           cbBancoID_dato =  cbBancoId.getSelectedItem().toString().trim();
           cbCuenta.removeAllItems();
           cbCuentaId.removeAllItems();
            
        }
        

        
        try {
            
           
            Rs = Sql.Select("select id,cuenta\n" +
                            "from bco_cuentas \n" +
                            "where banco="+ cbBancoID_dato);       
 
//            if(Sql.GetRowCount()>0)
//                btBuscar.setEnabled(true);
//            else
//                btBuscar.setEnabled(false);
            
            while(Rs.next()){
                if (cual ==2){
                    cbCuentaId2.addItem(Rs.getString("id"));
                cbCuenta2.addItem(Rs.getString("cuenta"));
                }
                else{
                    cbCuentaId.addItem(Rs.getString("id"));
                cbCuenta.addItem(Rs.getString("cuenta"));
                }
                
            }
        } catch (Exception e) {
            System.out.println(e.getMessage());
        } finally{
            Sql.Close();
        }
    }
    private void btCargaCartolaActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btCargaCartolaActionPerformed
        ExeSql Sql = new ExeSql();
        ResultSet Rs;
        String Query="";
        SimpleDateFormat sdf = new SimpleDateFormat("dd-MM-yyyy");
        DefaultTableModel Modelo1 = (DefaultTableModel) GrillaCartola.getModel();
        
        String tabla = "bco_cartola";
        String select = "select ba.nombre, ca.fecha,ca.oficina,ca.nrodocumento,ca.detalle,ca.cargo,ca.abono,ca.id,ca.asociado";
        
        fmMain.LimpiaGrilla(Modelo1);
       
        if(cbCuentaId.getSelectedItem().equals("5") || cbCuentaId.getSelectedItem().equals("7"))
            {
                tabla = "bco_cartola_tc";
                select = "select ba.nombre, ca.fechafac ,ca.ciudad|| ' - ' || ca.ncuota ||'/'|| ca.tcuotas as cuotas,ca.codigo ,ca.descripcion,ca.cargo,ca.abono,ca.id,ca.asociado";
            } 
        else{
            tabla = "bco_cartola";
            select = "select ba.nombre, ca.fecha,ca.oficina,ca.nrodocumento,ca.detalle,ca.cargo,ca.abono,ca.id,ca.asociado";
        }
                
        try {
            Query = select +" \n" +
                "from "+ tabla +" ca\n" +
                "left join bco_cuentas cu on ca.cuenta=cu.id\n" +
                "left join bco_bancos ba on ba.id = cu.banco\n" + 
                "where cu.id=" + cbCuentaId.getSelectedItem().toString().trim() + "\n" ;
            
            if(chkFecha.isSelected()){
                Query = Query + "and ca.fecha = '" + sdf.format(Fecha.getDate()) +"'\n";
            }
            
            if(cbConciliacion.getSelectedIndex()==0)
                Query = Query + "and ca.asociado = false \n";
            else if(cbConciliacion.getSelectedIndex()==1)
                Query = Query + "and ca.asociado = true \n";
            
            Query = Query + " Order by ca.id";
            
            Rs = Sql.Select(Query);
            
            while(Rs.next()){
                if(cbCuentaId.getSelectedItem().equals("5") || cbCuentaId.getSelectedItem().equals("7"))
                {
                Modelo1.addRow(new Object[]{
                                Rs.getString("Fechafac").trim(),
                                Rs.getString("cuotas").trim(),
                                Rs.getString("codigo").trim(),
                                Rs.getString("descripcion").trim(),
                                Rs.getInt("Cargo"),
                                Rs.getInt("abono"),
                                Rs.getBoolean("asociado"),
                                Rs.getInt("id")
                    
                });
                    
                }
                else{
                Modelo1.addRow(new Object[]{
                                Rs.getString("Fecha").trim(),
                                Rs.getString("Oficina").trim(),
                                Rs.getString("nrodocumento").trim(),
                                Rs.getString("detalle").trim(),
                                Rs.getInt("Cargo"),
                                Rs.getInt("abono"),
                                Rs.getBoolean("asociado"),
                                Rs.getInt("id")
                    
                });
                }
            }
        } catch (Exception e) {
        } finally{
            Sql.Close();
                    
        }
    }//GEN-LAST:event_btCargaCartolaActionPerformed
