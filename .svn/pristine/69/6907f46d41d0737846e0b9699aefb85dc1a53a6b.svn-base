    public void TarjetaCredito(){
        if (chkTarjeta.isSelected())
            {
            cbBanco2.setEnabled(false);
            cbCuenta2.setEnabled(false);
            Fecha2.setEnabled(false);
            cbConciliacion2.setEnabled(false);
            cbConciliacion.setEnabled(false);
            
            chkFecha2.setSelected(true);
            chkFecha2.setEnabled(false);
            
            
            cbBanco2.setSelectedIndex(cbBanco.getSelectedIndex());
            cbConciliacion2.setSelectedIndex(0);
            cbConciliacion.setSelectedIndex(0);
            Fecha2.setDate(Fecha.getDate());
            btCargaCartola2.doClick();
            btCargaCartola2.setEnabled(false);
            
            }
        else{
            cbBanco2.setEnabled(true);
            cbCuenta2.setEnabled(true);
            Fecha2.setEnabled(false);
            cbConciliacion2.setEnabled(true);
            chkFecha2.setSelected(false);
            chkFecha2.setEnabled(true);
            btCargaCartola2.setEnabled(true);
        }
       
    }

